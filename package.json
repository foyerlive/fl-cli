{
  "name": "fl-cli",
  "version": "1.0.2",
  "description": "FoyerLive CLI",
  "scripts": {
    "install": "npm run build:clean && npm run build:lib && npm run build:bin",
    "build:clean": "rimraf bin",
    "build:lib": "NODE_ENV=production babel src/lib --out-dir lib",
    "build:bin": "NODE_ENV=production babel src/bin --out-dir bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foyerlive/fl-cli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foyerlive/fl-cli/issues"
  },
  "homepage": "https://github.com/foyerlive/fl-cli#readme",
  "bin": "./bin/cli.js",
  "dependencies": {
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.14.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "cli-prompt": "^0.5.0",
    "commander": "^2.9.0",
    "es6-promise": "^3.1.2",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.15.0",
    "express": "^4.13.4",
    "form-data": "^0.2.0",
    "node-fetch": "^1.3.3",
    "rimraf": "^2.5.3",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
