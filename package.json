{
  "name": "fl-cli",
  "version": "1.1.3",
  "description": "FoyerLive CLI",
  "scripts": {
    "install": "npm run build:lib",
    "install:local": "npm run build:clean && npm run build:lib && npm run build:bin",
    "build:clean": "rimraf bin",
    "build:lib": "cross-env NODE_ENV=production babel src/lib --out-dir lib",
    "build:bin": "cross-env NODE_ENV=production babel src/bin --out-dir bin",
    "start:lib": "NODE_ENV=production babel --watch src/lib --out-dir lib",
    "start:bin": "NODE_ENV=production babel --watch src/bin --out-dir bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foyerlive/fl-cli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foyerlive/fl-cli/issues"
  },
  "homepage": "https://github.com/foyerlive/fl-cli#readme",
  "bin": "./bin/cli.js",
  "main": "lib/index.js",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "cli-prompt": "^0.6.0",
    "commander": "^2.9.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.28.0",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "^0.6.4",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "form-data": "^2.1.2",
    "jsx-control-statements": "^3.1.5",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "postcss-smart-import": "^0.6.10",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}
