{
  "name": "fl-cli",
  "version": "1.2.6",
  "description": "FoyerLive CLI",
  "scripts": {
    "postinstall:husky": "node node_modules/husky/lib/installer/bin install",
    "install": "npm run build:lib",
    "install:local": "npm run build:clean && npm run build:lib && npm run build:bin",
    "build:clean": "rimraf bin",
    "build:lib": "cross-env NODE_ENV=production babel src/lib --out-dir lib",
    "build:bin": "cross-env NODE_ENV=production babel src/bin --out-dir bin",
    "start:lib": "NODE_ENV=production babel --watch src/lib --out-dir lib",
    "start:bin": "NODE_ENV=production babel --watch src/bin --out-dir bin",
    "release": "semantic-release --no-ci",
    "commit": "git-cz",
    "cz": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foyerlive/fl-cli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foyerlive/fl-cli/issues"
  },
  "homepage": "https://github.com/foyerlive/fl-cli#readme",
  "bin": "./bin/cli.js",
  "main": "lib/index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "cli-prompt": "^0.6.0",
    "commander": "^2.19.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.28.11",
    "es6-promise": "^4.2.5",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "^0.6.4",
    "express": "^4.16.4",
    "file-loader": "^0.11.1",
    "form-data": "^2.3.3",
    "jsx-control-statements": "^3.2.8",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "postcss-smart-import": "^0.7.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.24.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "devDependencies": {
    "@foyer/commitizen-config": "^1.0.0",
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "cz-customizable-ghooks": "git+https://github.com/pramod-ie/cz-customizable-ghooks.git#master",
    "husky": "^3.0.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "node_modules/@foyer/commitizen-config"
    }
  }
}