{
  "name": "fl-cli",
  "version": "1.0.2",
  "description": "FoyerLive CLI",
  "scripts": {
    "install": "npm run build:clean && npm run build:lib && npm run build:bin",
    "build:clean": "rimraf bin",
    "build:lib": "NODE_ENV=production babel src/lib --out-dir lib",
    "build:bin": "NODE_ENV=production babel src/bin --out-dir bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foyerlive/fl-cli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foyerlive/fl-cli/issues"
  },
  "homepage": "https://github.com/foyerlive/fl-cli#readme",
  "bin": "./bin/cli.js",
  "main": "lib/index.js",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cli-prompt": "^0.6.0",
    "commander": "^2.9.0",
    "css-loader": "^0.26.2",
    "es6-promise": "^4.0.5",
    "eslint": "^3.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-react": "^6.10.0",
    "exports-loader": "^0.6.4",
    "express": "^4.14.1",
    "file-loader": "^0.10.1",
    "form-data": "^2.1.2",
    "jsx-control-statements": "^3.1.5",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "postcss-smart-import": "^0.6.9",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}
